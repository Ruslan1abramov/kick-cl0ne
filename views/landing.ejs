    <body>
    
    <div class="container">
        <% if(error && error.length > 0){ %>
            <div class="alert alert-danger" role="alert">
                <%= error %>
            </div>
        <% } %>
        <% if(success && success.length > 0){ %>
            <div class="alert alert-success" role="alert">
                <%= success %>
            </div>
        <% } %>
    </div>

    <div id="landing-header"  style="font-family: 'Permanent Marker', cursive;">
 		<h1>Welcome to KickCl0ne!</h1>
 		<h2>In the Background you can see some of our top projects</h2>
		<a href="/projects" class="btn btn-lg btn-success">View All Projects</a>
    </div>
